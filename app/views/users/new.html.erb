<h1>Sign Up</h1>


<%= render partial: 'layouts/errors', locals: {object: @user}  %>


<%= form_for @user do |f| %>

    <%= f.label :username %><br>
  <%= f.text_field :username %>

    <%= f.label :email %>
    <%= f.email_field :email %><br><br>


    <%= f.label :password %>
    <%= f.password_field :password %><br>
  <%= f.submit "Signup" %>
<% end %>
